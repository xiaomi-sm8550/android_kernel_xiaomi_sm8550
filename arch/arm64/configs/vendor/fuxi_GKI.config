# Fingerprint
CONFIG_FINGERPRINT_GOODIX_FOD=m
CONFIG_FINGERPRINT_FPC_1555=m
CONFIG_INPUT_FINGERPRINT=y

# IR
CONFIG_IR_SPI=m

# MTD
CONFIG_MTD=m
CONFIG_MTD_BLOCK=m
CONFIG_MTD_BLOCK2MTD=m
CONFIG_MTD_OOPS=m

# Power supply
CONFIG_MI_WIRELESS=y

# Product
CONFIG_TARGET_PRODUCT_FUXI=y

# Thermal
CONFIG_MI_THERMAL_INTERFACE=m

# Touchscreen
CONFIG_TOUCHSCREEN_ST_FTS_V521_SPI=m
CONFIG_TOUCHSCREEN_XIAOMI_TOUCHFEATURE=m

# Xiaomi HWID
CONFIG_MI_HARDWARE_ID=m

# CONFIG_QCOM_WCD_USBSS_I2C is not set
